{
  "compileOnSave": true,
  "include": [
      "src/**/*",
      "*.js"
, "eslint.config.mjs", "eslint.config.js"  ],
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noUnusedLocals": true
  }
}
